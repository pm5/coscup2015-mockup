{% extends "default.twig" %}

{% block intro %}
<div id="intro">
  <header id="header">
    <div class="wrap">
      <nav>
        <div id="logo"><a href="{{ baseUrl }}/"><img src="{{ baseUrl }}/images/logo.png"/></a></div>
        {% include "locale.twig" %}
      </nav>
    </div>
  </header>
  <div id="header-image"></div>
  <div id="features">
    <div class="wrap">
      {% include "events.twig" %}
      {% include "navbar.twig" %}
      {% include "archivebar.twig" %}
    </div>
  </div>
</div>
{% endblock %}

{% block content %}
  {{ content|raw }}
{% endblock %}
